{
  "name": "MattHub",
  "version": "0.4.0",
  "description": "Add ZenHub columns to the GitHub project view.",
  "author": "Matthew Walsh (matthew.walsh@consensys.net)",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://github.com/orgs/*/projects/*/views/*"],
      "js": ["content-script.js"],
      "css": ["style.css"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "web_accessible_resources": [
    "style.css"
  ],
  "content_security_policy": "worker-src github.com/assets-cdn/worker/ gist.github.com/assets-cdn/worker/ blob:;"
}
